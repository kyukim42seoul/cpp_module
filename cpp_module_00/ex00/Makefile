NAME = megaphone
CC = c++
CFLAGS = -Wall -Wextra -Werror
OBJECTS = megaphone.o

all : $(NAME) clean

$(NAME) : $(OBJECTS)
	$(CC) -o $(NAME) $(CFLAGS) $(OBJECTS)

clean :
	rm -f $(OBJECTS)

fclean : clean
	rm -f $(NAME)

re : fclean all

.PHONY : all clean fclean re
